import { api } from '../api';

export type Payload = {};

export type Response = {};

export const {{service}} = async (payload: Payload): Promise<Response | undefined> => {
  try {
    const { data } = await api.post('/post', payload);
    return data;
  } catch (error) {
    // errorAlert(error.response.data.message);
  }
};
