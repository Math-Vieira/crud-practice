import toast from 'react-hot-toast';
import { api } from '../';

type Response = {};

export const {{service}} = async (path: string): Promise<Response | undefined> => {
  try {
    const { data } = await api.get(`/${path}`);
    toast.success(data.message);

    return data;
  } catch (error: any) {
    toast.error(error.response.data.message);
  }
};
